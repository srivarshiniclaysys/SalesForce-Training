<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Due_Date_In_Past_Not_Allowed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Due_Date__c &lt; TODAY(), NOT(Bypass_Flow__c))</errorConditionFormula>
    <errorMessage>Due Date cannot be in the past</errorMessage>
</ValidationRule>
