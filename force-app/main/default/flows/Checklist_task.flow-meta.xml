    <formulas>
        <name>Checklist_Text</name>
        <dataType>String</dataType>
        <expression>LOWER(SUBSTITUTE({!Fetch_data.Loan_Review_Detail__c}, &quot;,&quot;,&quot;&quot;))</expression>
    </formulas>
    <formulas>
        <name>CleanCaseReview</name>
        <dataType>String</dataType>
        <expression>LOWER( SUBSTITUTE (TEXT( $Record.Loan_Review_Details__c),&quot;,&quot;,&quot;&quot;))</expression>
    </formulas>
    <formulas>
        <name>Percent_Formula</name>
        <dataType>Number</dataType>
        <expression>IF( {!TotalGeneratedCount} &gt; 0, ROUND( ( {!CompletedCount} / {!TotalGeneratedCount} ) * 100,0),0)</expression>
        <scale>2</scale>
    </formulas>
