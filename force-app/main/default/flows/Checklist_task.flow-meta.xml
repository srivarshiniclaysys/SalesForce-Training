    <decisions>
        <name>Does_the_selection_match_loan_review</name>
        <label>Does the selection match loan review?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Fetch_data</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Match_Loan_Review_Type</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Fetch_data.Loan_Review_Detail__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$Record.Loan_Review_Details__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Task_Field</targetReference>
            </connector>
            <label>Match Loan Review Type</label>
        </rules>
    </decisions>
    <decisions>
        <name>RecordType</name>
