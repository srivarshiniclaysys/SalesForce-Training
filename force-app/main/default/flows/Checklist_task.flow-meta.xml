        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>LoanReviewPage</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>RecordName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Loan Review</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Loan_Review_Details__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record__Prior.Loan_Review_Details__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Existing_Task</targetReference>
            </connector>
            <label>LoanReviewPage</label>
        </rules>
    </decisions>
